[
	{
		"id": 0,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "How was the bookstore?",
		"voice_file": "VOICE_SYS_001_635_100635",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "90362210238560528",
		"text": "I asked the person working there, but they didn't know any author by that name.",
		"voice_file": "VOICE_SYS_029_642_128642",
		"still_path_hash": "2239883848991404562",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "90362210238560528",
		"text": "I thought I would, at the very least, be able to find another book written by the same person.",
		"voice_file": "",
		"still_path_hash": "2239883848991404562",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Hmm... I wonder if my book was really rare.",
		"voice_file": "VOICE_SYS_001_636_100636",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "90362210238560528",
		"text": "Or maybe they simply haven't written anything new in years.",
		"voice_file": "VOICE_SYS_029_636_128636",
		"still_path_hash": "2239883848991404562",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "90362210238560528",
		"text": "I'll go look at the library, too.",
		"voice_file": "",
		"still_path_hash": "2239883848991404562",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh, okay! Thank you!",
		"voice_file": "VOICE_SYS_001_640_100640",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]