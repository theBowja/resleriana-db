[
	{
		"id": 0,
		"localized_name": "杰隆",
		"romanized_name": "Geron",
		"model_path_hash": "7036248754503774915",
		"text": "唔……",
		"voice_file": "VOICE_MAIN_003_011_00341",
		"still_path_hash": "6398968499954469286",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "瓦莱莉娅",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "你……为什么？！",
		"voice_file": "VOICE_MAIN_002_011_00342",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "杰隆",
		"romanized_name": "Geron",
		"model_path_hash": "7036248754503774915",
		"text": "不要东张西望……那家伙还活着！",
		"voice_file": "VOICE_MAIN_003_011_00343",
		"still_path_hash": "6398968499954469286",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "瓦莱莉娅",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "说得对，现在的敌人不是你。",
		"voice_file": "VOICE_MAIN_002_011_00344",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "瓦莱莉娅！你们没事吧？！",
		"voice_file": "VOICE_MAIN_001_011_00345",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "瓦莱莉娅",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "蕾斯娜！嗯，大家都没事。",
		"voice_file": "VOICE_MAIN_002_011_00346",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "0",
		"text": "（什么？力量在泄漏……？）",
		"voice_file": "VOICE_MAIN_001_011_00347",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "0",
		"text": "（……原来如此！\n　是刚才杰隆先生攻击过的地方！）",
		"voice_file": "VOICE_MAIN_001_011_00348",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "0",
		"text": "（如果能击中那里的话……！）",
		"voice_file": "VOICE_MAIN_001_011_00349",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "听我说，瓦莱莉娅！\n我想到一个主意。",
		"voice_file": "VOICE_MAIN_001_011_00350",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]