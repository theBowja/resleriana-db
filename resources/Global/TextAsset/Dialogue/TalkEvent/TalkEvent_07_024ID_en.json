[
	{
		"id": 0,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "0",
		"text": "?!",
		"voice_file": "VOICE_MAIN_049_007_00586",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Roman!",
		"voice_file": "VOICE_MAIN_001_007_00587",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Oh, it's you all.",
		"voice_file": "VOICE_MAIN_049_007_00588",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Marion",
		"romanized_name": "Marion",
		"model_path_hash": "2676271994623593349",
		"text": "Whoops, sorry about that. I couldn't tell who you were in this darkness.",
		"voice_file": "VOICE_MAIN_025_007_00589",
		"still_path_hash": "5397390121100149522",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "I feel like this has happened before, I think.",
		"voice_file": "VOICE_MAIN_049_007_00590",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh, I'm so glad you're all right, Roman!",
		"voice_file": "VOICE_MAIN_001_007_00591",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "We were so worried about you!",
		"voice_file": "VOICE_MAIN_048_007_00592",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Thanks. You all came to rescue me? I could cry right now.",
		"voice_file": "VOICE_MAIN_049_007_00593",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "I think we'd better save the talking for later.",
		"voice_file": "VOICE_MAIN_056_007_00594",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Bandit A",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "There he is! Don't let him get away!",
		"voice_file": "VOICE_MAIN_524_007_00595",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "Bandit B",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "You thought you could run from us?!",
		"voice_file": "VOICE_MAIN_525_007_00596",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "Yikes! There are so many of them!",
		"voice_file": "VOICE_MAIN_048_007_00597",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Now's not the time for a heart-to-heart! We have to get out of here, quick!",
		"voice_file": "VOICE_MAIN_049_007_00598",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Whoa! Roman?!",
		"voice_file": "VOICE_MAIN_001_007_00599",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 14,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Get in. We can leave them far behind in this thing.",
		"voice_file": "VOICE_MAIN_049_007_00600",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 15,
		"localized_name": "Marion",
		"romanized_name": "Marion",
		"model_path_hash": "2676271994623593349",
		"text": "You really expect us to ride a mining cart?!",
		"voice_file": "VOICE_MAIN_025_007_00601",
		"still_path_hash": "5397390121100149522",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 16,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Absolutely. All aboard!",
		"voice_file": "VOICE_MAIN_049_007_00602",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 17,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "Ahaha! We're flying! This is kind of fun, huh, Resna?",
		"voice_file": "VOICE_MAIN_048_007_00603",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 18,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Ahhh! My head is spinning! Whoa, oh, ohhh!",
		"voice_file": "VOICE_MAIN_001_007_00604",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 19,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "This will get us right to the mine's entrance. Just hold on a little lonâ€•",
		"voice_file": "VOICE_MAIN_049_007_00605",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 20,
		"localized_name": "Bandit B",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Stop right there!",
		"voice_file": "VOICE_MAIN_525_007_00606",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 21,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "They're catching up! We have to lose them!",
		"voice_file": "VOICE_MAIN_056_007_00607",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 22,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Roman! We're being followed!",
		"voice_file": "VOICE_MAIN_001_007_00608",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 23,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "Not to worry! There's a junction up ahead. Knock down this lever here, and... There!",
		"voice_file": "VOICE_MAIN_049_007_00609",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 24,
		"localized_name": "Bandit A",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "What in the world?! They're going a different way?! Curse youuu!",
		"voice_file": "VOICE_MAIN_524_007_00610",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 25,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "We've lost them! Great work!",
		"voice_file": "VOICE_MAIN_001_007_00611",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 26,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "All right, I think they're gone. Let's get off here.",
		"voice_file": "VOICE_MAIN_049_007_00612",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 27,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "We escaped without a hitch!",
		"voice_file": "VOICE_MAIN_048_007_00613",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 28,
		"localized_name": "Marion",
		"romanized_name": "Marion",
		"model_path_hash": "2676271994623593349",
		"text": "I wish that were true...",
		"voice_file": "VOICE_MAIN_025_007_00614",
		"still_path_hash": "5397390121100149522",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 29,
		"localized_name": "Roman",
		"romanized_name": "Roman",
		"model_path_hash": "2044084852602675005",
		"text": "That's...",
		"voice_file": "VOICE_MAIN_049_007_00615",
		"still_path_hash": "2419706081160656678",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 30,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Crow!",
		"voice_file": "VOICE_MAIN_001_007_00616",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]