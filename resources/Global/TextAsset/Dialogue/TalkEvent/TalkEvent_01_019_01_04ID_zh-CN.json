[
	{
		"id": 0,
		"localized_name": "海蒂",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "啊，你是那时候的……",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "你好。芙洛可小姐的失物交给她了吗？",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "海蒂",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "顺利转交啦。她很感谢你。",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "呵呵，那太好了！",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "对了，我想打听一件事，\n你要是知道些什么的话，希望你能告诉我……",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "你听没听说过一个叫阿拉尔的作家？",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "海蒂",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "阿拉尔……没听说过。",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "这样……",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "海蒂",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "抱歉啊，没帮上你的忙。",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "蕾斯娜",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "不会，还是谢谢你！\n我再去其他地方问问！",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]