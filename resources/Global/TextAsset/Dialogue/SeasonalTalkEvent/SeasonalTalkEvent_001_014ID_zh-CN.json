[
	{
		"id": 0,
		"localized_name": "苏菲",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "好，布置好了！",
		"voice_file": "VOICE_SYS_030_495_129495",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "普拉芙妲",
		"romanized_name": "Plachta",
		"model_path_hash": "3954629598630209020",
		"text": "希望它能顺我们心意乖乖上钩……",
		"voice_file": "VOICE_SYS_031_630_130630",
		"still_path_hash": "7115992466489359891",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "莱莎",
		"romanized_name": "Ryza",
		"model_path_hash": "6459971860569016995",
		"text": "你们两个快藏好。\n听说有人监视的话，万圣节噗尼就不会现身。",
		"voice_file": "VOICE_SYS_039_652_138652",
		"still_path_hash": "633958553226542878",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "普拉芙妲",
		"romanized_name": "Plachta",
		"model_path_hash": "3954629598630209020",
		"text": "我、我也明白……",
		"voice_file": "VOICE_SYS_031_648_130648",
		"still_path_hash": "7115992466489359891",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "万圣节噗尼",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "好香甜啊，这是什么气味？！\n本大爷从来没碰到过这么好吃的点心！",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "苏菲",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "它来了！",
		"voice_file": "VOICE_SYS_030_629_129629",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "普拉芙妲",
		"romanized_name": "Plachta",
		"model_path_hash": "3954629598630209020",
		"text": "真的出现了！",
		"voice_file": "VOICE_SYS_031_495_130495",
		"still_path_hash": "7115992466489359891",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "苏菲",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "让人好等！",
		"voice_file": "VOICE_SYS_030_655_129655",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "万圣节噗尼",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "什、什么情况？！",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "普拉芙妲",
		"romanized_name": "Plachta",
		"model_path_hash": "3954629598630209020",
		"text": "和我们一决胜负吧！",
		"voice_file": "VOICE_SYS_031_654_130654",
		"still_path_hash": "7115992466489359891",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "万圣节噗尼",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "难不成你们以为赢得了本大爷？！\n所有人准备好接受恶作剧的惩罚吧——！",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "苏菲",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "大家一起上！",
		"voice_file": "VOICE_SYS_030_111_129111",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	}
]