[
	{
		"id": 0,
		"localized_name": "Knight A",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "It's almost time! Stay sharp!",
		"voice_file": "VOICE_MAIN_539_017_00775",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Resna and the others will bring us the tool we need to fight the man-made Apexi! Stay strong!",
		"voice_file": "VOICE_MAIN_002_017_00776",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Until then, we will hold our ground!",
		"voice_file": "VOICE_MAIN_002_017_00777",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Everyone",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Yeaaah!",
		"voice_file": "VOICE_MAIN_051_017_00778",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Flocke",
		"romanized_name": "Flocke",
		"model_path_hash": "2379791261939253229",
		"text": "Leave the first aid to me!",
		"voice_file": "VOICE_MAIN_052_017_00782",
		"still_path_hash": "1324369732733195994",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Heidi, I need you to do something for me. I'm going to stay here and fight, so I need you to keep in touch with Resna.",
		"voice_file": "VOICE_MAIN_002_017_00783",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Got it! I'll do just that!",
		"voice_file": "VOICE_MAIN_051_017_00784",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Lanze",
		"romanized_name": "Lanze",
		"model_path_hash": "845404123422268284",
		"text": "Let's do this! I didn't get to face off with Weltex, but this time I'm goin' all in!",
		"voice_file": "VOICE_MAIN_053_017_00785",
		"still_path_hash": "5409134969144639524",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Lanze",
		"romanized_name": "Lanze",
		"model_path_hash": "845404123422268284",
		"text": "I ain't gonna let those Apexi get away with anything!",
		"voice_file": "VOICE_MAIN_053_017_00786",
		"still_path_hash": "5409134969144639524",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Knight B",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Sir Geron, thank you for your help in the last battle. Let us defeat the Apexi together once again!",
		"voice_file": "VOICE_MAIN_540_017_00787",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "Geron",
		"romanized_name": "Geron",
		"model_path_hash": "7036248754503774915",
		"text": "We will.",
		"voice_file": "VOICE_MAIN_003_017_00788",
		"still_path_hash": "6398968499954469286",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "Geron",
		"romanized_name": "Geron",
		"model_path_hash": "7036248754503774915",
		"text": "I will not let them destroy this place. I'll protect it—for Tim!",
		"voice_file": "VOICE_MAIN_003_017_00789",
		"still_path_hash": "6398968499954469286",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "Sir Sterk, thank you for your assistance.",
		"voice_file": "VOICE_MAIN_070_017_00790",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "Sterk",
		"romanized_name": "Sterk",
		"model_path_hash": "8185335709792791307",
		"text": "I hear this is your homeland, Lady Juna.",
		"voice_file": "VOICE_MAIN_074_017_00791",
		"still_path_hash": "1957504471001791451",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 14,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "It is, which is why I volunteered for this battlefield—to protect my homeland with my own hands.",
		"voice_file": "VOICE_MAIN_070_017_00792",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 15,
		"localized_name": "Sterk",
		"romanized_name": "Sterk",
		"model_path_hash": "8185335709792791307",
		"text": "A noble cause.",
		"voice_file": "VOICE_MAIN_074_017_00793",
		"still_path_hash": "1957504471001791451",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 16,
		"localized_name": "Sterk",
		"romanized_name": "Sterk",
		"model_path_hash": "8185335709792791307",
		"text": "I understand your desire to protect your homeland. As a fellow knight, I offer you my full support.",
		"voice_file": "VOICE_MAIN_074_017_00794",
		"still_path_hash": "1957504471001791451",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 17,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "Thank you. I'm glad you're here.",
		"voice_file": "VOICE_MAIN_070_017_00795",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 18,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "The battle will begin soon! Stay alert, everyone!",
		"voice_file": "VOICE_MAIN_070_017_00796",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 19,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Just...a little more!",
		"voice_file": "VOICE_MAIN_001_017_00797",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]