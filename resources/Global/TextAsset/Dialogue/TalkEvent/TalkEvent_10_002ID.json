[
	{
		"id": 0,
		"localized_name": "ヴァレリア",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "やぁ……っ！",
		"voice_file": "VOICE_MAIN_002_010_00015",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "はぁ……はぁ……！",
		"voice_file": "VOICE_MAIN_051_010_00016",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "ヴァレリア",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "みんな行った……？",
		"voice_file": "VOICE_MAIN_002_010_00017",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "ちゃんと逃げ切れたみたいだ！　私たちも逃げよう！",
		"voice_file": "VOICE_MAIN_051_010_00018",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "ヴァレリア",
		"romanized_name": "Valeria",
		"model_path_hash": "788127644417976526",
		"text": "そう……よかった……",
		"voice_file": "VOICE_MAIN_002_010_00019",
		"still_path_hash": "3829953996242913016",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "ちょ……ヴァレリア！？　ヴァレリア！",
		"voice_file": "VOICE_MAIN_051_010_00020",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "ヴァレリア！　しっかりしてよ！",
		"voice_file": "VOICE_MAIN_051_010_00021",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（どうする！　どうするハイディ……！）",
		"voice_file": "VOICE_MAIN_051_010_00022",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（辛い時、私はいつも逃げてばかりだった）",
		"voice_file": "VOICE_MAIN_051_010_00023",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "ねぇ、相棒",
		"voice_file": "VOICE_MAIN_051_010_00024",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（私は貴族の家の生まれだ）",
		"voice_file": "VOICE_MAIN_051_010_00025",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（ケンカだらけの家族が嫌になって逃げ出した）",
		"voice_file": "VOICE_MAIN_051_010_00026",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（シスターになったけど、堅苦しい生活に耐えられなくて\n　逃げ出した）",
		"voice_file": "VOICE_MAIN_051_010_00027",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（きったなくて小さな家だったけどさ）",
		"voice_file": "VOICE_MAIN_051_010_00028",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 14,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（あそこは、逃げてばかりの私が初めて手に入れた\n　安住の地だったんだ）",
		"voice_file": "VOICE_MAIN_051_010_00029",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 15,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "0",
		"text": "（フロッケと、ランツェのおっさんと、\n　ヴァレリア……あんた。私の大事な家族）",
		"voice_file": "VOICE_MAIN_051_010_00030",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 16,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "こんな私にもさ、大切なものができたんだ",
		"voice_file": "VOICE_MAIN_051_010_00031",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 17,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "だからさ",
		"voice_file": "VOICE_MAIN_051_010_00032",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 18,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "私はもう、逃げるわけにはいかない",
		"voice_file": "VOICE_MAIN_051_010_00033",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 19,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "私なりの戦い方で、やってやる！！",
		"voice_file": "VOICE_MAIN_051_010_00034",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 20,
		"localized_name": "ハイディ",
		"romanized_name": "Heidi",
		"model_path_hash": "4939855205667805900",
		"text": "やぁぁぁぁっ！",
		"voice_file": "VOICE_MAIN_051_010_00035",
		"still_path_hash": "4604195231108867445",
		"speech_balloon_type_id": 780292254
	}
]