[
	{
		"id": 0,
		"localized_name": "Escha",
		"romanized_name": "Escha",
		"model_path_hash": "4942920317009730088",
		"text": "Wow, Resna made these boots? It doesn't even feel like we're walking through snow!",
		"voice_file": "VOICE_MAIN_028_008_00575",
		"still_path_hash": "8567200502263774699",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "This is going to make getting around a breeze!",
		"voice_file": "VOICE_MAIN_051_008_00576",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Hey, you two! Don't just go off on your own!",
		"voice_file": "VOICE_MAIN_051_008_00577",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Huh? Is something wrong?",
		"voice_file": "VOICE_MAIN_002_008_00578",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Escha",
		"romanized_name": "Escha",
		"model_path_hash": "4942920317009730088",
		"text": "If you keep going at that pace, you're going to leave us behind.",
		"voice_file": "VOICE_MAIN_028_008_00579",
		"still_path_hash": "8567200502263774699",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Linca",
		"romanized_name": "Linca",
		"model_path_hash": "2320534114475787876",
		"text": "That wouldn't be good. Escha, could you maybe go a little faster?",
		"voice_file": "VOICE_MAIN_024_008_00580",
		"still_path_hash": "4464667044946317190",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Wait, what if we just carried you?",
		"voice_file": "VOICE_MAIN_002_008_00581",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Linca",
		"romanized_name": "Linca",
		"model_path_hash": "2320534114475787876",
		"text": "That's a great idea!",
		"voice_file": "VOICE_MAIN_024_008_00582",
		"still_path_hash": "4464667044946317190",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Stop agreeing with those sorts of things!",
		"voice_file": "VOICE_MAIN_051_008_00583",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Escha",
		"romanized_name": "Escha",
		"model_path_hash": "4942920317009730088",
		"text": "Anyway, we'd better stay on our guard. We still have a long way to go!",
		"voice_file": "VOICE_MAIN_028_008_00584",
		"still_path_hash": "8567200502263774699",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "Linca",
		"romanized_name": "Linca",
		"model_path_hash": "2320534114475787876",
		"text": "Escha always zones out when she's gathering, so get ready for her to be completely useless.",
		"voice_file": "VOICE_MAIN_024_008_00585",
		"still_path_hash": "4464667044946317190",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "Escha",
		"romanized_name": "Escha",
		"model_path_hash": "4942920317009730088",
		"text": "Whaaat?! Linca, that's just mean!",
		"voice_file": "VOICE_MAIN_028_008_00586",
		"still_path_hash": "8567200502263774699",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "Valeria",
		"romanized_name": "Valeria",
		"model_path_hash": "4979147540429202173",
		"text": "Ah, right. Let's see, we've got to collect...",
		"voice_file": "VOICE_MAIN_002_008_00587",
		"still_path_hash": "7558252374860649663",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "Escha",
		"romanized_name": "Escha",
		"model_path_hash": "4942920317009730088",
		"text": "Hmm...",
		"voice_file": "VOICE_MAIN_028_008_00588",
		"still_path_hash": "8567200502263774699",
		"speech_balloon_type_id": 780292254
	}
]