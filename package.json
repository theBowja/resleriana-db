{
  "name": "resleriana-db",
  "version": "0.0.0",
  "description": "Stores Atelier Resleriana data and hosts an API for it",
  "main": "main.mjs",
  "scripts": {
    "import": "cd import && node import.js",
    "updatealldata": "node auto/update.js",
    "updatePathHashMap": "node auto/runscripts.js --script updatePathHashMap",
    "extractImages": "node auto/runscripts.js --script extractImages",
    "extractText": "node auto/runscripts.js --script extractText",
    "extractBGM": "node auto/runscripts.js --script extractBGM",
    "extractVoice": "node auto/runscripts.js --script extractVoice",
    "downloadBundles": "node auto/runscripts.js --script downloadBundles"
  },
  "author": "theBowja",
  "license": "MIT",
  "dependencies": {
    "@leeoniya/ufuzzy": "^1.0.14"
  },
  "devDependencies": {
    "cloudinary": "^2.0.3",
    "json-bigint": "^1.0.0",
    "lz4": "^0.6.5",
    "msgpackr": "^1.10.1",
    "perfect-json": "^0.3.1",
    "yargs-parser": "^21.1.1"
  }
}
