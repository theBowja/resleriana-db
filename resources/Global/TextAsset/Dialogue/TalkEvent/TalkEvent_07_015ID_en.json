[
	{
		"id": 0,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Esty!",
		"voice_file": "VOICE_MAIN_001_007_00385",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "Hm? Well, if it isn't Resna and Izana. Are you journeying together today, Marion?",
		"voice_file": "VOICE_MAIN_056_007_00386",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Marion",
		"romanized_name": "Marion",
		"model_path_hash": "2676271994623593349",
		"text": "Yes, though we've found ourselves in a bit of a pickle.",
		"voice_file": "VOICE_MAIN_025_007_00387",
		"still_path_hash": "5397390121100149522",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh, so Esty is the one you were talking about asking to help us?",
		"voice_file": "VOICE_MAIN_001_007_00388",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "It'd be such a relief to have Esty with us!",
		"voice_file": "VOICE_MAIN_048_007_00389",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "Is there something the matter?",
		"voice_file": "VOICE_MAIN_056_007_00390",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Marion",
		"romanized_name": "Marion",
		"model_path_hash": "2676271994623593349",
		"text": "The thing is...",
		"voice_file": "VOICE_MAIN_025_007_00391",
		"still_path_hash": "5397390121100149522",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "I see. Of course I'll come along.",
		"voice_file": "VOICE_MAIN_056_007_00392",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Izana",
		"romanized_name": "Izana",
		"model_path_hash": "3955217441086782382",
		"text": "Yay!",
		"voice_file": "VOICE_MAIN_048_007_00393",
		"still_path_hash": "8711233837385589208",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Thank you so much, Esty!",
		"voice_file": "VOICE_MAIN_001_007_00394",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "We'd best hurry then. You said you were going to the mining site, right?",
		"voice_file": "VOICE_MAIN_056_007_00395",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "Esty",
		"romanized_name": "Esty",
		"model_path_hash": "5509121676670433180",
		"text": "We'll be rid of the kidnappers in no time. You can count on me!",
		"voice_file": "VOICE_MAIN_056_007_00396",
		"still_path_hash": "1666316267541402646",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "That's music to my ears! We really appreciate it!",
		"voice_file": "VOICE_MAIN_001_007_00397",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Roman, just you wait. We're on our way to rescue you!",
		"voice_file": "VOICE_MAIN_001_007_00398",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]