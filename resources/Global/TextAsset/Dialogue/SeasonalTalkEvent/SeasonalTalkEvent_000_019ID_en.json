[
	{
		"id": 0,
		"localized_name": "Meruru",
		"romanized_name": "Meruru",
		"model_path_hash": "160006433664293035",
		"text": "Hero! Over here!",
		"voice_file": "VOICE_SYS_021_120_120120",
		"still_path_hash": "4407416566126441531",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Ah. You have arrived.",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Lady Meruru! And the Demon King!",
		"voice_file": "VOICE_SYS_001_484_142484",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Quite the pathetic hero's party you are. I expected someone...stronger.",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Lent",
		"romanized_name": "Lent",
		"model_path_hash": "2885764269898401541",
		"text": "What did you say?!",
		"voice_file": "VOICE_SYS_040_654_139654",
		"still_path_hash": "2104807859548338143",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Raze",
		"romanized_name": "Raze",
		"model_path_hash": "7640478782351486010",
		"text": "Stop. He's just taunting us. No point in getting worked up.",
		"voice_file": "VOICE_SYS_017_106_116106",
		"still_path_hash": "9196008292396767773",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Tess",
		"romanized_name": "Tess",
		"model_path_hash": "7713659418971358664",
		"text": "That's right. He can see for himself how \"pathetic\" we are after we fight him!",
		"voice_file": "VOICE_SYS_035_085_134085",
		"still_path_hash": "1974319971304968741",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Demon King! I will defeat you and bring peace back to this world!",
		"voice_file": "VOICE_SYS_001_086_142086",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Rorona",
		"romanized_name": "Rorona",
		"model_path_hash": "4482406372396026577",
		"text": "Resna, use the Eternal Sword!",
		"voice_file": "VOICE_SYS_018_354_117354",
		"still_path_hash": "6911172049205438765",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "O Eternal Sword, lend me your strength! Drive away this evil force!",
		"voice_file": "VOICE_SYS_001_502_142502",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 10,
		"localized_name": "Meruru",
		"romanized_name": "Meruru",
		"model_path_hash": "160006433664293035",
		"text": "Thank you, all of you!",
		"voice_file": "VOICE_SYS_021_132_120132",
		"still_path_hash": "4407416566126441531",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 11,
		"localized_name": "Meruru",
		"romanized_name": "Meruru",
		"model_path_hash": "160006433664293035",
		"text": "Much of my magic has been spent, but I can attempt to seal the Demon King's power!",
		"voice_file": "VOICE_SYS_021_091_120091",
		"still_path_hash": "4407416566126441531",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 12,
		"localized_name": "Meruru",
		"romanized_name": "Meruru",
		"model_path_hash": "160006433664293035",
		"text": "I ask that you hold back the Demon King until I've gathered enough magic!",
		"voice_file": "VOICE_SYS_021_100_120100",
		"still_path_hash": "4407416566126441531",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 13,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Understood, Your Highness!",
		"voice_file": "VOICE_SYS_001_495_100495",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 14,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Hmph. Even with the princess at your side, you are nothing but insects to me.",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 15,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Once I have sent you all to your graves, all of humanity shall perish by my hand, and I shall take this world as my own!",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 16,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Do not underestimate us! Time to fight, Demon King!",
		"voice_file": "VOICE_SYS_001_085_142085",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 17,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "Hmph! Pitiful!",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 18,
		"localized_name": "Demon King",
		"romanized_name": "",
		"model_path_hash": "0",
		"text": "What in the...?!",
		"voice_file": "",
		"still_path_hash": "0",
		"speech_balloon_type_id": 780292254
	}
]