[
	{
		"id": 0,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Oh, hey, you're that girl I met earlier.",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Hello there. Did you get Flocke's bag to her safely?",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Sure did. She was really grateful.",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh good. Haha! That's a relief!",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "By the way, I don't know if you would know, but...",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Have you ever heard of an author named Allar?",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Allar? Sorry, doesn't ring a bell.",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh...",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "5270784299656074783",
		"text": "Sorry I couldn't help.",
		"voice_file": "",
		"still_path_hash": "120518867847693403",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 9,
		"localized_name": "Resna",
		"romanized_name": "Resna",
		"model_path_hash": "8643265264907274754",
		"text": "Oh, not at all. Thank you! I'll go ask somewhere else!",
		"voice_file": "",
		"still_path_hash": "3572566872346296899",
		"speech_balloon_type_id": 780292254
	}
]