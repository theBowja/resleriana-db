[
	{
		"id": 0,
		"localized_name": "Sophie",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "Wow...! What a lively town! Don't you think so, Plachta?",
		"voice_file": "VOICE_SYS_030_646_129646",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Plachta",
		"romanized_name": "Plachta",
		"model_path_hash": "894414068988709786",
		"text": "Sophie, would you mind keeping it down? You stick out like a sore thumb.",
		"voice_file": "VOICE_SYS_031_642_130642",
		"still_path_hash": "8944192761651257696",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Sophie",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "Ehehe! Sorry. So? Where should we go first?",
		"voice_file": "VOICE_SYS_030_628_129628",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Plachta",
		"romanized_name": "Plachta",
		"model_path_hash": "894414068988709786",
		"text": "Hm... A library or a bookstore, perhaps?",
		"voice_file": "VOICE_SYS_031_635_130635",
		"still_path_hash": "8944192761651257696",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Plachta",
		"romanized_name": "Plachta",
		"model_path_hash": "894414068988709786",
		"text": "If we want to learn more about this country, that would be the fastest way of going about it.",
		"voice_file": "VOICE_SYS_031_620_130620",
		"still_path_hash": "8944192761651257696",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Sophie",
		"romanized_name": "Sophie",
		"model_path_hash": "1747704331216962355",
		"text": "I was thinking the same thing! Okay, let's go find some books!",
		"voice_file": "VOICE_SYS_030_647_129647",
		"still_path_hash": "3373470695039518718",
		"speech_balloon_type_id": 780292254
	}
]