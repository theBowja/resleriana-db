[
	{
		"id": 0,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "8054053719409878813",
		"text": "It looks so dark in there. And the dragon could be anywhere... We'll have to be careful.",
		"voice_file": "",
		"still_path_hash": "7391924254937150345",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "?! Heidi, wait!",
		"voice_file": "",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "8054053719409878813",
		"text": "Huh?",
		"voice_file": "",
		"still_path_hash": "7391924254937150345",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Juna",
		"romanized_name": "Juna",
		"model_path_hash": "7176586596496405136",
		"text": "Something's coming!",
		"voice_file": "",
		"still_path_hash": "6403040722015916711",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Heidi",
		"romanized_name": "Heidi",
		"model_path_hash": "8054053719409878813",
		"text": "Is it the dragon?!",
		"voice_file": "",
		"still_path_hash": "7391924254937150345",
		"speech_balloon_type_id": 780292254
	},
	{
