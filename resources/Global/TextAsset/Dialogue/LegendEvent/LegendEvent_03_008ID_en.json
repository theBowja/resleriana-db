[
	{
		"id": 0,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "6412190799642563600",
		"text": "Wow, another abundant water source.",
		"voice_file": "",
		"still_path_hash": "4984234209229109354",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 1,
		"localized_name": "Shallotte",
		"romanized_name": "Shallotte",
		"model_path_hash": "322297020958821866",
		"text": "What a wonderful place, and the water's so clear. Let's try a sip!",
		"voice_file": "",
		"still_path_hash": "5588667421325366917",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 2,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "6412190799642563600",
		"text": "Mmm... How nice. Very refreshing.",
		"voice_file": "",
		"still_path_hash": "4984234209229109354",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 3,
		"localized_name": "Shallotte",
		"romanized_name": "Shallotte",
		"model_path_hash": "322297020958821866",
		"text": "Oh wow, you're right. This seems perfect for throat troubles. Hah!",
		"voice_file": "",
		"still_path_hash": "5588667421325366917",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 4,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "6412190799642563600",
		"text": "Yeeek! It's freezing! Hehe. I'll show you!",
		"voice_file": "",
		"still_path_hash": "4984234209229109354",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 5,
		"localized_name": "Shallotte",
		"romanized_name": "Shallotte",
		"model_path_hash": "322297020958821866",
		"text": "Hey! Come on, I didn't splash that much on you! All right, you asked for it!",
		"voice_file": "",
		"still_path_hash": "5588667421325366917",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 6,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "6412190799642563600",
		"text": "Ahaha! Lotte, it's really cold!",
		"voice_file": "",
		"still_path_hash": "4984234209229109354",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 7,
		"localized_name": "Shallistera",
		"romanized_name": "Shallistera",
		"model_path_hash": "6412190799642563600",
		"text": "Hehe. *Sigh* We couldn't even begin to imagine doing this in our world. It'd be so wasteful.",
		"voice_file": "",
		"still_path_hash": "4984234209229109354",
		"speech_balloon_type_id": 780292254
	},
	{
		"id": 8,
		"localized_name": "Shallotte",
		"romanized_name": "Shallotte",
		"model_path_hash": "322297020958821866",
		"text": "Yeah... But now's our chance! It's fine if we do it here in this world.",
		"voice_file": "",
		"still_path_hash": "5588667421325366917",
		"speech_balloon_type_id": 780292254
	}
